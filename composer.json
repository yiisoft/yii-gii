{
    "name": "yiisoft/yii-gii",
    "type": "library",
    "description": "Yii Framework Code Generator Extension",
    "keywords": [
        "yii",
        "gii",
        "code generator"
    ],
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/yiisoft/yii-gii",
        "issues": "https://github.com/yiisoft/yii-gii/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "phpspec/php-diff": "^1.1.0",
        "yiisoft/aliases": "^1.0",
        "yiisoft/arrays": "^3.0@dev",
        "yiisoft/http": "^1.0",
        "yiisoft/json": "^1.0",
        "yiisoft/strings": "^1.0",
        "yiisoft/validator": "^3.0@dev",
        "yiisoft/var-dumper": "^3.0@dev",
        "yiisoft/view": "^3.0@dev",
        "yiisoft/yii-console": "^3.0@dev"
    },
    "require-dev": {
        "phan/phan": "^3.0",
        "phpunit/phpunit": "^9.4",
        "roave/infection-static-analysis-plugin": "^1.5",
        "spatie/phpunit-watcher": "^1.23",
        "vimeo/psalm": "^4.2",
        "yiisoft/composer-config-plugin": "^1.0@dev",
        "yiisoft/di": "^3.0@dev",
        "yiisoft/event-dispatcher": "^3.0@dev",
        "yiisoft/files": "^3.0@dev",
        "yiisoft/log": "^3.0@dev"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Yii\\Gii\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Yii\\Gii\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        },
        "config-plugin": {
            "params": "config/params.php",
            "console": [
                "$common",
                "config/console.php"
            ],
            "common": "config/common.php",
            "tests": "config/tests.php"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit --testdox --no-interaction",
        "test-watch": "phpunit-watcher watch"
    }
}
